<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="AdminNavigateToNewWidgetPageActionGroup">
        <annotations>
            <description>Navigates to Admin New Widget page.</description>
        </annotations>
        <amOnPage url="{{AdminNewWidgetPage.url}}" stepKey="navigateToNewWidgetPage"/>
        <waitForPageLoad stepKey="waitForNewWidgetPageLoad"/>
    </actionGroup>
</actionGroups>

